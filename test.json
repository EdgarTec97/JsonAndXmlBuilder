{
  "soap:Envelope": {
    "@xmlns:soap": "{{safeVal 'http://www.w3.org/2003/05/soap-envelope' 'http://www.w3.org/2003/05/soap-envelope' }}",
    "@xmlns:tem": "{{safeVal 'http://tempuri.org/' 'http://tempuri.org/' }}",
    "soap:Header": {
      "tem:AuthHeader": {
        "tem:Nombre_Cargue": {
          "#text": "{{safeVal extraParams.servientrega.nombreCargue 'AJA_GROUP' }}"
        }
      }
    },
    "soap:Body": {
      "tem:CargueMasivoExterno": {
        "tem:arrayGuias": {
          "tem:string": {
            "#text": "{{safeVal extraParams.servientrega.string '?' }}"
          }
        }
      }
    }
  }
}
